@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema>
    a skos:ConceptScheme ;
    skos:prefLabel "Conceptscheme type van informatievoorziening" ;
    skos:definition "Codelijst voor type van informatievoorziening." .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/Energie> 
    a skos:Concept ;
    skos:prefLabel "Energie" ;
    skos:definition "Basisuitleg met betrekking tot verbruik, efficiëntie, isolatie, hernieuwbare energie, restwarmte, ventilatie of andere mogelijkheden om de gebouweenheid zuiniger te maken." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/BasisWoonkwaliteit> 
    a skos:Concept ;
    skos:prefLabel "Basis Woonkwaliteit" ;
    skos:definition "Basisuitleg met betrekking tot het gedrag van de bewoner en bijbehorende algemene gedragsmaatregelen ter verbetering van de gebouweenheid, alsook over woongerelateerde onderwerpen met betrekking op veiligheid, gezondheid en woonkwaliteit, zoals verwaarlozing en leegstand." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/Klimaatadaptatie> 
    a skos:Concept ;
    skos:prefLabel "Klimaatadaptatie" ;
    skos:definition "Basisuitleg over maatregelen, die inspelen op het aanpassen van de gebouweenheid aan de huidige en toekomstige klimaatverandering." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/Premie> 
    a skos:Concept ;
    skos:prefLabel "Premie" ;
    skos:definition "Basisuitleg over het aanbod aan financiële vergoedingen voor het doorlopen van een actie." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/Lening> 
    a skos:Concept ;
    skos:prefLabel "Lening" ;
    skos:definition "Basisuitleg over de verstrekking van geldbedragen tegen bepaalde rentevoet en op een bepaalde termijn " ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/Doorverwijzing> 
    a skos:Concept ;
    skos:prefLabel "Doorverwijzing" ;
    skos:definition "Doorsturen naar een gespecialiseerde dienst, binnen of buiten het de publieke organisatie." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningThema/(Sociale)Huuraangelegenheden> 
    a skos:Concept ;
    skos:prefLabel "(Sociale)Huuraangelegenheden" ;
    skos:definition "Generieke informatie omtrent alle huuractiviteiten, van zowel standpunt van de huurder als verhuurder." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningThema> .
