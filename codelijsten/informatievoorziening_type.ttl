@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType>
    a skos:ConceptScheme ;
    skos:prefLabel "Conceptscheme type van informatievoorziening" ;
    skos:definition "Codelijst voor type van informatievoorziening." .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/Energie> 
    a skos:Concept ;
    skos:prefLabel "Energie" ;
    skos:definition "Basisuitleg met betrekking tot verbruik, efficiëntie, isolatie of andere mogelijkheden om het pand zuiniger te maken." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/BasisWoonkwaliteit> 
    a skos:Concept ;
    skos:prefLabel "Basis Woonkwaliteit" ;
    skos:definition "Basisuitleg met betrekking tot het gedrag van de bewoner en bijbehorende algemene gedragsmaatregelen ter verbetering van het pand, net als basisuitleg om de woningkwaliteit te verbeteren (door bijvoorbeeld vocht of schimmel aan te nemen.)" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/Klimaatadaptatie> 
    a skos:Concept ;
    skos:prefLabel "Klimaatadaptatie" ;
    skos:definition "Basisuitleg over maatregelen, die inspelen op de huidige en toekomstige klimaatverandering." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/Premie> 
    a skos:Concept ;
    skos:prefLabel "Premie" ;
    skos:definition "Basisuitleg over het aanbod aan financiële vergoedingen na afloop van een renovatie." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/Lening> 
    a skos:Concept ;
    skos:prefLabel "Lening" ;
    skos:definition "Basisuitleg over de verstrekking van geldbedragen tegen bepaalde rentevoet en op een bepaalde termijn " ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .

<https://data.vlaanderen.be/id/concept/InformatievoorzieningType/Doorverwijzing> 
    a skos:Concept ;
    skos:prefLabel "Doorverwijzing" ;
    skos:definition "Doorsturen naar een gespecialiseerde dienst, binnen of buiten het energiehuis." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/InformatievoorzieningType> .
